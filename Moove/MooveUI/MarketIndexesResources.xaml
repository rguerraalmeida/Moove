<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="IndexUnchanged" Color="#FF373737"/>
    <SolidColorBrush x:Key="IndexPositiveLowChange" Color="#FF188D00"/>
    <SolidColorBrush x:Key="IndexPositiveHighChange" Color="#FF9EFF8A"/>
    <SolidColorBrush x:Key="IndexNegativeLowChange" Color="#FFB60303"/>
    <SolidColorBrush x:Key="IndexNegativeHighChange" Color="#FFEE3942"/>
    <SolidColorBrush x:Key="IndexHeaderLabelColor" Color="#18000000"/>
    <SolidColorBrush x:Key="IndexValueLabelColor" Color="#66000000"/>


    <Style x:Key="IndexHeaderLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="{DynamicResource IndexHeaderLabelColor}" />
        <Setter Property="Background" Value="{x:Null}" />
        <Setter Property="Padding" Value="5,0,0,0" />

    </Style>

    <Style x:Key="IndexValueLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="{DynamicResource IndexValueLabelColor}" />
        <Setter Property="Background" Value="{x:Null}" />
        <Setter Property="Padding" Value="5,0,0,0" />
    </Style>
    
</ResourceDictionary>